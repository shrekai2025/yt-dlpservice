{
  "youtube": {
    "name": "YouTube",
    "domains": [
      "youtube.com",
      "www.youtube.com",
      "youtu.be",
      "m.youtube.com"
    ],
    "urlPatterns": [
      "/watch\\?v=",
      "/embed/",
      "/v/",
      "youtu.be/"
    ],
    "contentTypes": ["video"],
    "requiresAuth": false,
    "downloadMethod": "yt-dlp"
  },
  "bilibili": {
    "name": "哔哩哔哩",
    "domains": [
      "bilibili.com",
      "www.bilibili.com",
      "m.bilibili.com",
      "b23.tv"
    ],
    "urlPatterns": [
      "^https?://(www\\.)?bilibili\\.com/video/BV[a-zA-Z0-9]+",
      "^https?://m\\.bilibili\\.com/video/BV[a-zA-Z0-9]+",
      "^https?://b23\\.tv/[a-zA-Z0-9]+"
    ],
    "contentTypes": ["video"],
    "requiresAuth": false,
    "downloadMethod": "yt-dlp"
  },
  "xiaoyuzhou": {
    "name": "小宇宙",
    "domains": [
      "xiaoyuzhoufm.com",
      "www.xiaoyuzhoufm.com"
    ],
    "urlPatterns": [
      "^https?://(www\\.)?xiaoyuzhoufm\\.com/episode/[a-zA-Z0-9]+"
    ],
    "contentTypes": ["audio", "podcast"],
    "requiresAuth": false,
    "downloadMethod": "custom",
    "note": "小宇宙平台仅支持单集下载 (/episode/)，不支持播客主页 (/podcast/)"
  },
  "applepodcasts": {
    "name": "Apple播客",
    "domains": [
      "podcasts.apple.com"
    ],
    "urlPatterns": [
      "^https?://podcasts\\.apple\\.com/[a-z]{2}/podcast/.+/id\\d+\\?.*i=\\d+"
    ],
    "contentTypes": ["audio", "podcast"],
    "requiresAuth": false,
    "downloadMethod": "custom",
    "note": "Apple播客使用网页解析方式下载音频，自动选择最小音质以减少文件大小"
  }
} 