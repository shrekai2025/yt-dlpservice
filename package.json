{
  "name": "yt-dlpservice",
  "version": "0.1.0",
  "description": "基于 yt-dlp 和通义听悟 API 的在线视频内容提取工具",
  "type": "module",
  "private": true,
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/yt-dlpservice.git"
  },
  "keywords": [
    "yt-dlp",
    "video-download",
    "speech-to-text",
    "tingwu",
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset",
    "db:seed": "npx tsx prisma/seed-ai-generation.ts",
    "ai:setup": "bash scripts/setup-ai-generation.sh",
    "test:scraper": "npx tsx scripts/test-metadata-scraper.ts",
    "youtube:auth": "node scripts/youtube-auth-cli.js",
    "youtube:test": "node scripts/test-youtube-cookie.js",
    "youtube:status": "node scripts/youtube-auth-cli.js --status",
    "youtube:setup": "node scripts/youtube-auth-cli.js --set-cookie",
    "lint": "next lint",
    "clean": "rm -rf .next tsconfig.tsbuildinfo",
    "clean:full": "bash scripts/clean-cache.sh",
    "clean:cache": "rm -rf .next node_modules/.cache .turbo 2>/dev/null || true",
    "dev:clean": "npm run clean:cache && npm run dev",
    "postinstall": "prisma generate && npm run setup:ytdlp",
    "setup:ytdlp": "python3 -m pip install --upgrade yt-dlp --break-system-packages || pipx install yt-dlp || echo 'Please install yt-dlp manually'",
    "update:ytdlp": "python3 -m pip install --upgrade yt-dlp --break-system-packages || pipx upgrade yt-dlp || echo 'Please update yt-dlp manually'",
    "deploy:setup": "npm install && npm run setup:ytdlp && npm run db:push"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@trpc/client": "^11.4.3",
    "@trpc/next": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "@types/archiver": "^6.0.4",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@volcengine/openapi": "^1.30.2",
    "@volcengine/tos-sdk": "^2.7.5",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.545.0",
    "next": "^15.2.3",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "puppeteer": "^24.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^4.1.11",
    "tar": "^7.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "npm@10.9.2"
}
